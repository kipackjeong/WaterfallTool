    "firebase": "^11.4.0",
    "framer-motion": "^11.0.28",
    "fs": "^0.0.1-security",
    "idb": "^8.0.2",
    "lodash": "^4.17.21",
    "mssql": "^11.0.1",
    "next": "13.1.2",
    "next-auth": "^4.22.1",
    "next-themes": "^0.4.4",
    "npx": "^10.2.2",
