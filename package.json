{
  "private": true,
  "name": "waterfalltool",
  "description": "",
  "version": "0.0.1",
  "author": "Kipack Jeong <kipackjeong@outlook.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/kipackjeong/WaterfallTool"
  },
  "main": "main/index.js",
  "scripts": {
    "dev:web": "cd renderer && next dev",
    "dev:all": "yarn dev:desktop && yarn dev:server",
    "dev:desktop": "yarn build:desktop && electron . && yarn dev:watch",
    "dev:server": "yarn build:server && cd server && yarn start:dev",
    "dev:watch": "concurrently \"yarn watch:main\" \"yarn watch:renderer\" \"yarn watch:electron\"",
    "watch:main": "tsc -w -p main",
    "watch:renderer": "next dev renderer --port 8888",
    "watch:electron": "wait-on http://localhost:8888 && electron-reload . --electron-path node_modules/.bin/electron -- .",
    "build:desktop": "yarn clean && yarn build:electron",
    "dist": "yarn build:all && electron-builder",
    "build:electron": "yarn install && cd renderer && next build && cd .. && tsc -p main",
    "build:server": "cd server && yarn install && yarn build",
    "build:all": "yarn clean && yarn build:server && yarn build:electron",
    "clean": "sudo rm -rf dist renderer/.next main/*.js main/**/*.js server/dist",
    "prod:server": "cd server && yarn clean && yarn build && yarn start:prod"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.1.1",
    "@chakra-ui/react": "^2.8.2",
    "@chakra-ui/theme-tools": "^2.1.2",
